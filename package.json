{
  "name": "Shopmon",
  "version": "1.0.0",
  "description": "Shopmon is an online sales management system that provides business owners with the ability to setup online sales company and manage their sales.",
  "main": "index.js",
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha --exit --timeout 15000 --require babel-register ./server/tests/**/*.test.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls && nyc report --reporter=lcov",
    "start": "babel-node ./server/index.js",
    "start:dev": "nodemon --exec babel-node ./server/index.js",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chukwuemekachm/Shopmon.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chukwuemekachm/Shopmon/issues"
  },
  "homepage": "https://github.com/chukwuemekachm/Shopmon#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "mocha": "^5.2.0",
    "morgan": "^1.9.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.38.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "cli": "^1.0.1",
    "coveralls": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "nodemon": "^1.18.3",
    "nyc": "^12.0.2",
    "sequelize-cli": "^4.0.0"
  }
}
